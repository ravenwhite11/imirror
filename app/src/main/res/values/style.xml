<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="style0_15dp">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/design_default_color_on_secondary</item>
    </style>
    <style name="style1_red_25dp">
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">#DA6C6C</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="style2_link">
        <item name="android:textColor">#7DD0F6</item>
    </style>

    <!--加載動畫屬性-->
    <style name="dialogStyle" parent="Theme.AppCompat.Dialog">
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="dialogWindowAnim" parent="android:Animation" mce_bogus="1">
        <item name="android:windowEnterAnimation">@anim/dialog_enter</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit</item>
    </style>


</resources>


